<rtest name="mmd/path-cycle">

  <keywords> builtin fast elastic mixed </keywords>

  <link_files>
    base.exdiff {NAME}.rxml {NAME}.xml {NAME}.base_exo
  </link_files>

  <execute name="mmd"> {NAME}.xml </execute>

  <execute name="exdiff">
    -f base.exdiff {NAME}.exo {NAME}.base_exo
  </execute>

</rtest>

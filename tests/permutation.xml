<GMDSpec>

  <Permutation method="zip" seed="42" correlation="table plot">
    <ResponseFunction href="gmd.max(PRESSURE)" descriptor="PRES"/>
    <Permutate var="K" values="weibull(125.e9, 14, 3)"/>
    <Permutate var="G" values="percentage(45.e9, 10, 3)"/>
  </Permutation>

  <Physics>

    <Path type="prdef" kappa="0" tstar="1" estar="-.5" amplitude="1" nfac="1000" ratfac="1">
      <!-- termination time, number of steps, control, Cij -->
      0 0 222222 0 0 0 0 0 0
      1 1 222222 1 0 0 0 0 0
      2 1 222222 2 0 0 0 0 0
      3 1 222222 1 0 0 0 0 0
      4 1 222222 0 0 0 0 0 0
    </Path>

    <Material density="5" model="elastic">
      <K> {K} </K>
      <G> {G} </G>
    </Material>

  </Physics>

</GMDSpec>

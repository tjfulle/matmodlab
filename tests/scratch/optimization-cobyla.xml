<MMLSpec>

  <Optimization method="cobyla" maxiter="25" tolerance="1e-4">
    <ResponseFunction function="opt-sig-v-time" descriptor="SIG_V_TIME"/>
    <AuxiliaryFile href="opt-baseline.dat"/>
    <Optimize var="opt_k" initial_value="129.e9"/>
    <Optimize var="opt_g" initial_value="54.e9"/>
  </Optimization>

  <Physics>

    <Material model="elastic">
      <G> {opt_g} </G>  <!-- 53.e9  value used to create exmpls.tbl -->
      <K> {opt_k} </K>  <!-- 135.e9 value used to create exmpls.tbl -->
    </Material>

    <Path type="prdef" format="table" cols="1 3:8" cfmt="222222" tfmt="time"
	  href="exmpls.tbl"/>

  </Physics>
</MMLSpec>
